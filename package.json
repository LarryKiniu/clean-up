{
  "name": "clean-up",
  "version": "1.0.1",
  "description": "A tool to help clean up files in a directory",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "change": "yarn beachball",
    "check": "yarn beachball check --branch origin/main --since main --kep-change-files --no-fetch",
    "bump": "yarn beachball bump --branch origin/main --since main --kep-change-files --no-fetch"
  },
  "author": "Larry Kiniu",
  "license": "MIT",
  "devDependencies": {
    "beachball": "^2.10.2"
  }
}
